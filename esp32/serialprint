#include <WiFi.h>

const char* ssid="Aasquare";
const char* password="ThomasAndFriends";

void setup() {
  // put your setup code here, to run once:
Serial.begin(115200);
WiFi.begin(ssid,password);
while(WiFi.status() !=WL_CONNECTED);
{
  delay(100);
  Serial.println(".");
}
Serial.println(WiFi.localIP());
}

